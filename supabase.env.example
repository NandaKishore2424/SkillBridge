SUPABASE_DB_HOST=<your-supabase-host>
SUPABASE_DB_PORT=5432
SUPABASE_DB_NAME=<your-database-name>
SUPABASE_DB_USER=<your-database-user>
SUPABASE_DB_PASSWORD=<your-database-password>
SUPABASE_DB_SSLMODE=require
SUPABASE_DB_POOL_SIZE=10
SUPABASE_DB_MIN_IDLE=5
SUPABASE_JDBC_URL=jdbc:postgresql://<your-supabase-host>:5432/<your-database-name>?sslmode=require

# Optional Spring overrides
SPRING_JPA_HIBERNATE_DDL_AUTO=validate
SPRING_SQL_INIT_MODE=never
SERVER_PORT=8080

# JWT configuration placeholders
JWT_SECRET=<generate-32-byte-secret>
JWT_EXPIRATION=86400000

